<h1>Listing client_titles</h1>

<table>
  <tr>
    <th>Client</th>
    <th>Title</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @client_titles.each do |client_title| %>
  <tr>
    <td><%= client_title.client_id %></td>
    <td><%= client_title.title_id %></td>
    <td><%= link_to 'Show', client_title %></td>
    <td><%= link_to 'Edit', edit_client_title_path(client_title) %></td>
    <td><%= link_to 'Destroy', client_title, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

