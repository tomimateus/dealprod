<body id="page-micuenta">

<!--header-->
<header id="top">
  <div class="container">
    <div class="row">
      <div class="span3" id="logo-header"><a href="/offers"><img src="/assets/logo-header.png"></a></div>

    </div><!--end row-->
  </div><!--end container header-->
</header><!--end header-->



<div class="container" id="main-content">
  <div class="row">
    <section class="span12">
      <header class="out">
        <h2>Olvidaste tu contraseña?</h2>
      </header>

      <div class="wrapper-padding">

        <div class="wrapper">
          <article class="row-fluid">
            <div class="span8">

              <%= form_for(resource, :as => resource_name, :url => password_path(resource_name), :html => { :method => :put }) do |f| %>
                  <%= devise_error_messages! %>
                  <fieldset>

                    <%= f.hidden_field :reset_password_token %>

                    <p>Completá los siguientes datos y obtené tu nueva contraseña</p>
                    <hr>
                    <div class="row-fluid">
                      <div class="span6">
                        <label>Nueva Contraseña</label>
                        <%= f.password_field :password, :label => false, :class => "input-xlarge", :style => "margin-right:10px;" %>

                        <label>Repetir Contraseña</label>
                        <%= f.password_field :password_confirmation, :label => false, :class => "input-xlarge", :style => "margin-right:10px;" %>

                      </div>

                    </div>


                    <hr>
                    <div class="text-center">
                      <button type="submit" class="btn btn-dealhunter rounded-borders-btn">Cambiar mi contraseña</button>
                    </div>
                  </fieldset>
              <% end %>
            </div> <!--end span8-->
            <div class="span3 offset1">
              <address>
                <strong>Dirección</strong><br>
                Malabia 1720, Piso 2<br>
                Capital Federal, CP 1414<br>
                <abbr title="Phone"></abbr>
              </address>

              <address>
                <strong>Email</strong><br>
                <a href="mailto:info@dealhunter.com">info@dealhunter.com</a>
              </address>

              <address>
                <strong>Teléfono</strong><br>
                <p>(+54)-11-5365-5921</p>
              </address>

              <address>
                <strong>Twitter</strong><br>
                <p>Para obtener respuestas en tiempo real a todas tus preguntas agreganos en twitter <a href="https://twitter.com/dealhunterArg" target="_blank">@dealhunterArg</a></p>
              </address>

            </div><!--end span4-->

          </article>

        </div><!--end wrapper-->
      </div><!--end wrapper-padding-->
    </section><!--end main content-->
  </div><!--end container-->
</div><!--end row-->


<!--footer-->
